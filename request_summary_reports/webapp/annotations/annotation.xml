<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/HTML5.xml">
        <edmx:Include Namespace="com.sap.vocabularies.HTML5.v1" Alias="HTML5"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Namespace="com.sap.vocabularies.Common.v1" Alias="Common"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI.xml">
        <edmx:Include Namespace="com.sap.vocabularies.UI.v1" Alias="UI"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Capabilities.V1.xml">
        <edmx:Include Namespace="Org.OData.Capabilities.V1" Alias="Capabilities"/>
    </edmx:Reference>
    <edmx:Reference Uri="/odata/v2/HumanResourceService/$metadata">
        <edmx:Include Namespace="HumanResourceService"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="com.sal.reports.requests">
            <Annotations Target="HumanResourceService.SF_Country">
                <Annotation Term="Common.Label" String="{@i18n>Country Code}"/>
                <Annotation Term="Common.ValueList">
                    <Record Type="Common.ValueListType">
                        <PropertyValue Property="CollectionPath" String="SF_Country"/>
                        <PropertyValue Property="Parameters">
                            <Collection>
                                <Record Type="Common.ValueListParameterInOut">
                                    <PropertyValue Property="LocalDataProperty" PropertyPath="code"/>
                                    <PropertyValue Property="ValueListProperty" String="externalName_defaultValue"/>
                                </Record>
                            </Collection>
                        </PropertyValue>
                    </Record>
                </Annotation>
                <Annotation Term="Common.ValueListWithFixedValues"/>
            </Annotations>
            <Annotations Target="HumanResourceService.Tickets">
                <!-- For Restrictions -->
                <Annotation Term="Capabilities.InsertRestrictions">
                    <Record Type="Capabilities.InsertRestrictionsType">
                        <PropertyValue Property="Insertable" Bool="true"/>
                    </Record>
                </Annotation>
                <Annotation Term="Capabilities.DeleteRestrictions">
                    <Record Type="Capabilities.DeleteRestrictionsType">
                        <PropertyValue Property="Deletable" Bool="true"/>
                    </Record>
                </Annotation>
                <Annotation Term="Capabilities.UpdateRestrictions">
                    <Record Type="Capabilities.UpdateRestrictionsType">
                        <PropertyValue Property="Updatable" Bool="false"/>
                    </Record>
                </Annotation>
                <!-- For List Page Filters -->
                <Annotation Term="UI.SelectionFields">
                    <Collection>
                        <PropertyPath>moduleId</PropertyPath>
                        <PropertyPath>ticketCode</PropertyPath>
                        <PropertyPath>effectiveStartDate</PropertyPath>
                        <PropertyPath>externalCode</PropertyPath>
                        <PropertyPath>status</PropertyPath>
                    </Collection>
                </Annotation>
                <!-- For List Items Object Attribute -->
                <Annotation Term="Common.SemanticKey">
                    <Collection>
                        <PropertyPath>ticketCode</PropertyPath>
                    </Collection>
                </Annotation>
                <!-- For List Header Toolbar tiltle -->
                <Annotation Term="UI.HeaderInfo">
                    <Record Type="UI.HeaderInfoType">
                        <PropertyValue Property="TypeName" String="{@i18n>Requests}"/>
                        <PropertyValue Property="TypeNamePlural" String="{@i18n>Requests}"/>
                    </Record>
                </Annotation>
                <!-- For Displaying the List -->
                <Annotation Term="UI.LineItem">
                    <!-- For Setting Criticality of item, i.e Highlight color  -->
                    <Annotation Term="UI.Criticality" Path="subModuleId"/>

                    <!-- For columns to be visible on table -->
                    <Collection>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="ticketCode"/>
                            <PropertyValue Property="Label" String="{@i18n>Ticket Code}"/>
                            <!-- For Adding CSS property width to set width to table column -->
                            <Annotation Term="com.sap.vocabularies.HTML5.v1.CssDefaults">
                                <Record Type="HTML5.CssDefaultsType">
                                    <PropertyValue Property="width" String="6rem"/>
                                </Record>
                            </Annotation>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="ID"/>
                            <PropertyValue Property="Label" String="{@i18n>ID}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="moduleId"/>
                            <PropertyValue Property="Label" String="{@i18n>Module ID}"/>
                            <Annotation Term="com.sap.vocabularies.HTML5.v1.CssDefaults">
                                <Record Type="HTML5.CssDefaultsType">
                                    <PropertyValue Property="width" String="5rem"/>
                                </Record>
                            </Annotation>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="subModuleId"/>
                            <PropertyValue Property="Label" String="{@i18n>Sub Module ID}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="effectiveStartDate"/>
                            <PropertyValue Property="Label" String="{@i18n>Effective Start Date}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="requestDate"/>
                            <PropertyValue Property="Label" String="{@i18n>Request Date}"/>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="status"/>
                            <PropertyValue Property="Label" String="{@i18n>Status}"/>
                            <Annotation Term="UI.DataPoint">
                                <Record Type="HTML5.DataPointType">
                                    <PropertyValue Property="CriticalityRepresentation" EnumType="WithoutIcon">
                                    </PropertyValue>
                                </Record>
                            </Annotation>
                            <Annotation Term="com.sap.vocabularies.HTML5.v1.CssDefaults">
                                <Record Type="HTML5.CssDefaultsType">
                                    <PropertyValue Property="width" String="9rem"/>
                                </Record>
                            </Annotation>
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="summary"/>
                            <PropertyValue Property="Label" String="{@i18n>Summary}"/>
                            <Annotation Term="com.sap.vocabularies.HTML5.v1.CssDefaults">
                                <Record Type="HTML5.CssDefaultsType">
                                    <PropertyValue Property="width" String="30rem"/>
                                </Record>
                            </Annotation>
                        </Record>
                    </Collection>
                </Annotation>
            </Annotations>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
<core:FragmentDefinition xmlns="sap.m" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:core="sap.ui.core"
	xmlns:filterBar="sap.ui.comp.filterbar" xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml">
	<Dialog
		title="{parts:[{path:'i18n>ED'},{path:'/Purchaserequisitionitemtext'},{path:'/Material'}],formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatDisplayTxt'}"
		contentWidth="20%" id="editDialog">
		<l:Grid id="editGrid" hSpacing="1" vSpacing="0" defaultSpan="L6 M6 S6" width="100%">
			<l:content>
				<Text text="" width="100%" class="marginForText"/>
				<Text text="" width="100%" class="marginForText"/>
				<Text text="{i18n>Pro_Sor}:" textAlign="Center" width="120%" class="marginForText"/>
				<Link press="handleSoSDialog" id="linkSOSEditFgt"
					text="{parts:[{path:'/Sos_count'},{path:'i18n>SOS'}],formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatDisplayText'}"
					enabled="{path: '/Sos_count', formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatLinkDisable'}" width="80%"
					class="marginForText"/>
				<Input value="{i18n>SUP}:" width="100%" textAlign="End" class="marginForText" editable="false"/>
				<MultiInput id="supplierF4multiInput" placeholder="{i18n>Supplier}"
					value="{parts:[{path:'/Fixedvendor'},{path:'/Supplier'}],formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatSupplierText'}"
					width="80%" valueHelpRequest="onValueHelpRequest" suppportMultiSelect="true" liveChange="onChangeSupplier"
					enabled="{path: '/Purchasingdocumentitemcategory', formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatDocItemCategory'}"/>
				<Input value="{i18n>DD}:" width="100%" textAlign="End" class="marginForText" editable="false"/>
				<DatePicker id="Date" dateValue="{path:'/Deliverydate'}" change="onDateChanged" width="80%"
					valueState="{path: '/Deliverydate',formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatDateValidation'}"/>
				<Input value="{i18n>QUANT}:" width="100%" textAlign="End" class="marginForText" editable="false"/>
				<l:Grid id="editGridQuantity" hSpacing="0" vSpacing="1" defaultSpan="L6 M6 S6">
					<l:content>
						<Input id="idEditQuantity" value="{path: '/Quantity'}"
							enabled="{path: '/Purchasingdocumentitemcategory', formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatDocItemCategory'}"
							liveChange="onInputQuantityChanged" maxLength="10"/>
						<Input id="idInputQuantity" value="{/Materialbaseunit}" class="marginForInput" width="95%" editable="false"/>
					</l:content>
				</l:Grid>
			</l:content>
		</l:Grid>
		<beginButton>
			<Button text="{i18n>OK}" app:dialogType="Std" press="onDialogOkButton"
				enabled="{path: '/Deliverydate',formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatDateOkButton'}" width="20%"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>Cancel}" app:dialogType="Std" press="onEditDialogCloseButton" width="20%"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>
<core:FragmentDefinition
  xmlns="sap.m" xmlns:l="sap.ui.layout" 
  controllerName="ui.ssuite.s2p.mm.pur.pr.prcss.s1.view.S1"
  xmlns:core="sap.ui.core">
  
<SelectDialog 
        noDataText="{i18n>NoDataText}"
        title="{parts:[{path:'i18n>SoSTitleOne'},{path:'i18n>SoSTitleTwo'}],formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatitext'}"
        rememberSelections = 'true'
           contentWidth="40%"
           id="idList"
           contentHeight="40%"
           search="onSearch"
           confirm="onSosConfirm"
           close="handleClose"
           items="{path: '/SourceOfSupplySet' }">
           
                         <StandardListItem
                           id = "StandardListItemId"
                           title="{parts:[{path:'NAME'},{path:'LIFNR'}],formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatAcct'}"
                           info="{parts:[{path:'i18n>TOTAL'},{path:'GRPR'},{path:'WAERS'}],
                           formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatSoSTotalPrice'}"
                           description="{parts:[{path:'i18n>Infr'},{path:'INFNR'},{path:'i18n>Contract'},{path:'EBELN'}], formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatSosInfoCtr'}"
                           type="Active" />
  </SelectDialog>
</core:FragmentDefinition>
<core:View controllerName="ui.ssuite.s2p.mm.pur.pr.prcss.s1.view.S4" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout" xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"
	xmlns:table="sap.ui.table" xmlns="sap.m">
	<Page class="sapUiFioriObjectPage" id="RFQPreviewPage" navButtonPress="onNavBack" showNavButton="true" title="{i18n>RFQVIEW}">
		<content>
			<!--<Panel class="sapUiMediumMarginBegin sapUiMediumMarginEnd" width="auto">-->
			<!--	<content>-->
			<!--<headerToolbar>-->
			<!--</headerToolbar>-->
			<f:SimpleForm maxContainerCols="2">
				<f:toolbar>
					<Toolbar>
						<Title text="{i18n>REQGENINFO}"/>
					</Toolbar>
				</f:toolbar>
				<f:content>
					<Label text="{i18n>RFQTPE}" class="sapUiSizeCompact sapUiTinyMarginTop"/>
					<ComboBox change="onRFQTypeChange" class="sapUiSizeCompact" id='idRFQTypes' selectedKey="{DraftRFQHeader>/Purchasingdocumenttype}"
						items="{DraftRFQType>/results}" width="200px" selectionChange="onChangeRFQDraft">
						<core:Item key="{DraftRFQType>RFQType}" text="{DraftRFQType>RFQType_Text}"/>
					</ComboBox>
					<Label text="{i18n>RFQDESC}" class="sapUiSizeCompact sapUiTinyMarginTop"/>
					<Input class="sapUiSizeCompact" id="idRFQDescription" value="{DraftRFQHeader>/Requestforquotationname}" height="30px" width="200px"
						change="onChangeRFQDraft"/>
					<!--<Label text="{i18n>RFQSDATE}" class="sapUiSizeCompact sapUiTinyMarginTop"/>-->
					<!--<DatePicker class="sapUiSizeCompact" id="idRFQDate"-->
					<!--	value="{path:'DraftRFQHeader>/Quotationearliestsubmsndate' , type:'sap.ui.model.type.Date', formatOptions: { style: 'medium', strictParsing: true}}"-->
					<!--	 valueFormat="dd/MM/yyyy" width="200px" change="onChangeRFQDraft"/>-->
					<Label text="{i18n>RFQSUBMISSION}" class="sapUiSizeCompact sapUiTinyMarginTop"/>
					<DatePicker class="sapUiSizeCompact" id="idRFQSubmission"
						value="{path:'DraftRFQHeader>/Quotationlatestsubmissiondate' ,type:'sap.ui.model.type.Date', formatOptions: { style: 'medium', strictParsing: true}}"
						valueFormat="dd/MM/yyyy" width="200px" change="onChangeRFQDraft"/>
					<Label text="{i18n>PURGRP}" class="sapUiSizeCompact sapUiTinyMarginTop"/>
					<Input class="sapUiSizeCompact" demandPopin="true"
						value="{parts:[{path:'DraftRFQHeader>/Purchasinggrouptext'},{path:'DraftRFQHeader>/Purchasinggroup'}], formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatPurOrg'}"
						width="200px" editable="false"/>
					<Label text="{i18n>PURORG}" class="sapUiSizeCompact sapUiTinyMarginTop"/>
					<Input class="sapUiSizeCompact" demandPopin="true"
						value="{parts:[{path:'DraftRFQHeader>/Purchasingorganizationtext'},{path:'DraftRFQHeader>/Purchasingorganization'}], formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatPurOrg'}"
						width="200px" editable="false"/>
				</f:content>
			</f:SimpleForm>
			<!--	</content>-->
			<!--</Panel>-->
			<!--<Panel class="sapUiMediumMarginBegin sapUiMediumMarginEnd" width="auto">-->
			<!--<content>-->
			<Table delete="handleBidderDelete" items="{DraftRFQBidder>/results}" noDataText="{i18n>NoInvitedBidder}" mode="Delete" showSeparators="All">
				<headerToolbar>
					<Toolbar>
						<Title text="{i18n>invitebidder}"/>
						<ToolbarSpacer/>
						<Button icon="sap-icon://add" id="addBidders" press="onPressAddBidder" tooltip="{i18n>addBidder}"/>
					</Toolbar>
				</headerToolbar>
				<columns design="Transparent">
					<Column hAlign="Begin" id="colBidderCompany" vAlign="Middle">
						<Text text="{i18n>RFQCMP}"/>
					</Column>
					<Column hAlign="Begin" id="colBidderContact" vAlign="Middle">
						<Text text="{i18n>Address}"/>
					</Column>
					<Column hAlign="Begin" id="colBidderCountry" vAlign="Middle">
						<Text text="{i18n>RFQCOUN}"/>
					</Column>
					<Column hAlign="Begin" id="colBidderemail" vAlign="Middle">
						<Text text="{i18n>Email}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<!--	<Text id="bidderCompany" text="{DraftRFQBidder>Supplier}"/>
						<VBox>
							<Label text="{DraftRFQBidder>Suppliername}"/>
							<Label text="{DraftRFQBidder>Phonenumber}"/>
							<Label text="{DraftRFQBidder>Emailaddress}"/>
						</VBox>
						<Text id="bidderCountry" text="{DraftRFQBidder>Country}"/>
						<Link id="bidderCompID" text="{DraftRFQBidder>Emailaddress}"/>-->
						<ObjectIdentifier text="{DraftRFQBidder>Supplier}" title="{DraftRFQBidder>Suppliername}"/>
						<Text id="bidderAddress" text="{DraftRFQBidder>Addressid}"/>
						<Text id="bidderCountry" text="{DraftRFQBidder>Country}"/>
						<Link id="bidderCompID" text="{DraftRFQBidder>Emailaddress}" press="onPressEmailBidder"/>
					</ColumnListItem>
				</items>
			</Table>
			<!--</content>-->
			<!--</Panel>-->
			<!--<Panel class="sapUiMediumMarginBegin sapUiMediumMarginEnd" width="auto">-->
			<!--<content>-->
			<Table delete="handleRFQDelete" id="RFQTable" items="{DraftRFQItem>/results}" mode="Delete" showSeparators="All">
				<headerToolbar>
					<Toolbar>
						<Title text="{i18n>PRITEM}"/>
					</Toolbar>
				</headerToolbar>
				<columns design="Transparent">
					<Column demandPopin="true" hAlign="Begin" id="RFQItemNo" vAlign="Middle" minScreenWidth="Desktop">
						<Text text="{i18n>RFQITM}"/>
					</Column>
					<Column demandPopin="true" hAlign="Begin" id="RFQDescription" vAlign="Middle" minScreenWidth="Desktop">
						<Text text="{i18n>DESC}"/>
					</Column>
					<Column demandPopin="true" hAlign="Begin" id="RFQMatGroup" vAlign="Middle" minScreenWidth="Desktop">
						<Text text="{i18n>REQMATGRP}"/>
					</Column>
					<Column demandPopin="true" hAlign="End" id="RFQQuantity" vAlign="Middle" minScreenWidth="Desktop">
						<Text text="{i18n>REQQUAN}"/>
					</Column>
					<Column demandPopin="true" hAlign="Center" id="DeliveryDate" vAlign="Middle" minScreenWidth="Desktop">
						<Text text="{i18n>DELIVDATE}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<Link id="colRFQItemNo"
							text="{parts:[{path:'DraftRFQItem>Purchaserequisition'},{path:'DraftRFQItem>Purchaserequisitionitem'}], formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatPrNumItmNum'}"
							press="handleNav"/>
						<Text id="colRFQDesc" text="{DraftRFQItem>Purchasingdocumentitemtext}"/>
						<Text id="colRFQMatGrp"
							text="{parts:[{path:'DraftRFQItem>Materialgrouptext'},{path:'DraftRFQItem>Materialgroup'}], formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatPurOrg'}"/>
						<Text id="colRFQQuan"
							text="{parts:[{path:'DraftRFQItem>Schedulelineorderquantity'},{path:'DraftRFQItem>Orderquantityunit'}], formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatQuantity'}"/>
						<Text id="colDelDate"
							text="{parts:[{path:'DraftRFQItem>Schedulelinedeliverydate'}], formatter:'ui.ssuite.s2p.mm.pur.pr.prcss.s1.util.Formatter.formatDateRead'}"/>
					</ColumnListItem>
				</items>
			</Table>
			<!--</content>-->
			<!--</Panel>-->
		</content>
		<footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button type="Emphasized" enabled="true" id='idRFQPublish' press='fnRFQPublish' text="{i18n>PUBLISH}"/>
				<Button enabled="true" id='idRFQSave' press='fnRFQSave' text="{i18n>PRCS}"/>
			</Toolbar>
		</footer>
	</Page>
</core:View>
<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="mySelectDialog"
	
		title="{FragmetModel>/titleName}"
		search=".onSearch"
		confirm=".onConfirmPendingRequest"
		cancel=".onDialogClose"
        items="{path: 'FragmetModel>/'}"
		>
		<StandardListItem
        	title="{FragmetModel>ticketCode}"
			description="{FragmetModel>summary}"
			 />
	</SelectDialog>
</core:FragmentDefinition>
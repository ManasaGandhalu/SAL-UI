<mvc:View
    controllerName="com.sal.salhr.controller.MasterList"
    xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc"
    xmlns:ui="sap.ui.table"
>
   <f:DynamicPage id="dynamicPageId" toggleHeaderOnTitleClick="false">
	
		<f:content>
			<VBox fitContainer="true" id="idV1">
			
				 <ui:Table id="idTb"
				rows="{/MasterSubModules}"
				selectionMode="Single"
                
				visibleRowCount="7"
				paste="onPaste"
				ariaLabelledBy="title"
                >
					<ui:extension>
					<OverflowToolbar style="Clear">
                    <ToolbarSpacer/>
							<SearchField width="20%" />
					</OverflowToolbar>
				</ui:extension>
				<ui:columns>
					<ui:Column width="25rem">
						<Label text="Type" />
						<ui:template>
							<Text text="{name}" wrapping="false" />
						</ui:template>
					</ui:Column>
				</ui:columns>
			</ui:Table> 

                       <!-- <List items="{path: '/ParentLineItemSet'}" updateFinished="onUpdateListFinished">
                <headerToolbar>
                    <OverflowToolbar>
                    <content>
                    <Title text="{i18n>parentItems}"/>
                    </content>
                    </OverflowToolbar>
                </headerToolbar>
                <StandardListItem title="{Name}" description="Description: {Description}" info="Material Code: {MaterialCode}" type="Navigation" press="onParentLineItemPress"/>
            </List>-->
			</VBox>
		</f:content>

		
	</f:DynamicPage>
</mvc:View>